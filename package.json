{
  "name": "appium-ecp",
  "version": "0.1.0",
  "description": "Client interface for Roku",
  "main": "./build/index.js",
  "scripts": {
    "clean": "rm -rf node_modules && rm -f package-lock.json && npm install",
    "prepare": "gulp prepublish",
    "test": "gulp once",
    "watch": "gulp watch",
    "build": "gulp transpile",
    "mocha": "mocha",
    "e2e-test": "gulp transpile && npm run mocha -- -t 600000 --recursive build/test/functional/",
    "coverage": "gulp coveralls",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "precommit-test": "REPORTER=dot gulp once",
    "lint": "gulp lint",
    "lint:fix": "gulp eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sharkyStudy/appium-ecp.git"
  },
  "author": "Sharky Study <sharky.study@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/sharkyStudy/appium-ecp/issues"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=6"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "files": [
    "index.js",
    "lib",
    "build/index.js",
    "build/lib"
  ],
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "appium-support": "^2.4.0",
    "axios": "^0.21.1",
    "form-data": "^4.0.0",
    "fs": "^0.0.1-security",
    "indigestion": "^0.1.4",
    "jszip": "^3.6.0",
    "libxmljs2": "^0.26.7",
    "lodash": "^4.17.11",
    "node-fetch": "^2.6.1",
    "source-map-support": "^0.5.9",
    "teen_process": "^1.3.1",
    "url": "^0.11.0",
    "xml-js": "1.6.11"
  },
  "pre-commit": [
    "precommit-msg",
    "precommit-test"
  ],
  "devDependencies": {
    "@types/node": "^17.0.41",
    "@types/node-fetch": "^2.5.8",
    "ajv": "^8.0.1",
    "appium-gulp-plugins": "^5.5.0",
    "appium-test-support": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "chai": "^4.0.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.22.0",
    "eslint-config-appium": "^4.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-mocha": "^8.1.0",
    "eslint-plugin-promise": "^4.0.0",
    "gulp": "^4.0.2",
    "mocha": "^8.3.2",
    "pre-commit": "^1.1.3"
  },
  "homepage": "https://github.com/sharkyStudy/appium-ecp#readme",
  "keywords": [
    "client",
    "interface",
    "appium",
    "Roku"
  ]
}
